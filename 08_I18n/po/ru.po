#: src/guessnum.c
msgid "Think of a number from 1 to 100, then press Enter."
msgstr "Загадайте число от 1 до 100 и нажмите Enter."

#: src/guessnum.c
msgid "Is your number greater than %d? (\"%s\"/\"%s\"): "
msgstr "Ваше число больше %d? (\"%s\"/\"%s\"): "

#: src/guessnum.c
msgid "Yes"
msgstr "Да"

#: src/guessnum.c
msgid "No"
msgstr "Нет"

#: src/guessnum.c
msgid "\nError: input closed (EOF). Aborting.\n"
msgstr "\nОшибка: ввод закрыт (EOF). Прерывание.\n"

#: src/guessnum.c
msgid "Invalid answer. Please type exactly \"%s\" or \"%s\".\n"
msgstr "Неверный ответ. Введите строго \"%s\" или \"%s\".\n"

#: src/guessnum.c
msgid "Guessed! Your number is %d.\n"
msgstr "Угадал! Ваше число — %d.\n"
